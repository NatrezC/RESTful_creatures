<h1>List of Prehistoric Creatures</h1>

<form method="GET" action="/prehistoric_creatures">
    <label for="typeFilter">Filter By Name:</label>
    <input type="text" id="typeFilter" name="type">
    <input type="submit">
</form>
<ul>
    <% preCreatures.forEach((p, i)=>{ %>
        <li>
            <%= p.type %> 
            <img src="<%=p.img_url %>"/>
            <form method="POST" action="/prehistoric_creatures/<%= i %>/?_method=DELETE">
                <input type="submit" value="Delete">
        </li>
    <% }) %> 
</ul>